        vy += ay * dt;                       {\cf20 // See: http://en.wikipedia.org/wiki/Backward_Euler_method}\par
\par
         x += vx * dt;\par
         y += vy * dt;\par
\par
        {\cf19 if} (x > 800) \{ vx = -vx; x = 800; \}  {\cf20 // = 800 is not the precise solution. Can you make it better?}\par
        {\cf19 if} (x <   0) \{ vx = -vx; x =   0; \}\par
        {\cf19 if} (y > 600) \{ vy = -vy; y = 600; \}\par
        {\cf19 if} (y <   0) \{ vy = -vy; y =   0; \}\par
\par
        {\cf19 if} (txGetAsyncKeyState (VK_LEFT))  vx--;\par
        {\cf19 if} (txGetAsyncKeyState (VK_RIGHT)) vx++;\par
        {\cf19 if} (txGetAsyncKeyState (VK_UP))    vy--;\par
        {\cf19 if} (txGetAsyncKeyState (VK_DOWN))  vy++;\par
        {\cf19 if} (txGetAsyncKeyState (VK_SPACE)) vx = vy = 0;\par
        {\cf19 if} (txGetAsyncKeyState ({\cf23 'M'}))      printf ({\cf22 "Meow "});\par
\par
        txSleep (20);\par
        \}\par
    \}\par
}
