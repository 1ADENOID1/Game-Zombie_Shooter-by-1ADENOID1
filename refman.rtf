\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf18 int} main()\par
    \{\par
    txCreateWindow (800, 600);\par
\par
    POINT size = txGetExtent();\par
\par
    txSetFillColor (TX_BLACK);\par
    txTextCursor ({\cf17 false});\par
    txBegin();\par
\par
    HDC src = GetDC (HWND_DESKTOP);                       {\cf20 // Get HDC from Windows Desktop}\par
\par
    RGBQUAD* buf = NULL;                                  {\cf20 // Do NOT actually create the array!}\par
    HDC dc = txCreateDIBSection (size.x, size.y, &buf);\par
    assert (dc); assert (buf);                            {\cf20 // Here, 'buf' points to an array created}\par
                                                          {\cf20 // by txCreateDIBSection()}\par
    {\cf19 while} (!GetAsyncKeyState (VK_ESCAPE))\par
        \{\par
        txBitBlt (dc, 0, 0, size.x, size.y, src);\par
\par
        {\cf19 for} ({\cf18 int} y = 0; y < size.y; y++)\par
        {\cf19 for} ({\cf18 int} x = 0; x < size.x; x++)\par
            \{\par
            RGBQUAD* c = & buf [x + y * size.x];          {\cf20 // Get color at (x, y) within image buffer}\par
\par
            c->rgbRed      = (BYTE) c->rgbRed;\par
            c->rgbGreen    = (BYTE) c->rgbGreen;\par
            c->rgbBlue     = (BYTE) c->rgbBlue;\par
\par
            {\cf18 double} r       = hypot (x - size.x/2, y - size.y/2);\par
            {\cf18 double} alpha   = sin (0.05 * r) * 0.1 + 0.5;\par
\par
            c->rgbReserved = (BYTE) ROUND (255 * alpha);  {\cf20 // Set alpha-channel (transparency)}\par
            \}\par
\par
        txUseAlpha (dc) asserted;                         {\cf20 // Premultiply colors with alpha}\par
\par
        txClear();\par
        txAlphaBlend (txDC(), 0, 0, 0, 0, dc);\par
\par
        printf ({\cf22 "FPS %.0lf\\t\\t\\r"}, txGetFPS());\par
        txSleep (0);\par
        \}\par
\par
    txSaveImage ({\cf22 "TxFilter.bmp"}, dc);\par
\par
    txDeleteDC (dc);\par
    ReleaseDC (HWND_DESKTOP, src);                        {\cf20 // Free Windows Desktop HDC}\par
\par
    {\cf19 return} 0;\par
    \}\par
}
